<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.finalproject.bttd.mybatismapper.UserBatisMapper">


    <select id="getUserDetails" resultType="com.finalproject.bttd.dto.UserDetailDto">
        select user_id, user_name, user_age, user_weight, user_win, user_lose, photo from user where user_id = #{user_id};

    </select>

</mapper>